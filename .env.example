# Copy this file to .env and fill in your values

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Set to true to initialize pools with liquidity during deployment
INITIALIZE_POOLS=false

# Set to true to enable gas reporting during tests
REPORT_GAS=false

# Helios Testnet Configuration (already configured in hardhat.config.js)
# RPC_URL=https://testnet1.helioschainlabs.org/
# CHAIN_ID=42000

# Optional: Etherscan API key for contract verification
ETHERSCAN_API_KEY=dummy_api_key